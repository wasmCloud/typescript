{
  /* Visit https://aka.ms/tsconfig to read more about this file */
  "compilerOptions": {
    /* Language and Environment */
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,

    /* Source code */
    "paths": {},
    "allowJs": false,

    /* Generation */
    "outDir": "./dist",
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true
  }
}
