{
  "name": "washboard-ui",
  "private": true,
  "version": "0.7.1",
  "type": "module",
  "scripts": {
    "dev": "vite --clearScreen false",
    "build": "yarn build:tsc && yarn build:vite",
    "build:vite": "vite build",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc -w",
    "preview": "vite preview",
    "lint": "yarn lint:eslint",
    "lint:fix": "yarn lint:eslint:fix",
    "lint:eslint": "eslint src tests --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "test:e2e": "yarn test:e2e:setup && yarn test:playwright:headless",
    "test:e2e:setup": "yarn playwright install --with-deps chromium",
    "test:playwright:headless": "playwright test",
    "test:playwright:ui": "playwright test --ui",
    "format": "yarn format:prettier",
    "format:fix": "yarn format:prettier:fix",
    "format:prettier": "prettier --ignore-unknown --no-error-on-unmatched-pattern '**/*' --check",
    "format:prettier:fix": "yarn format:prettier --write"
  },
  "dependencies": {
